<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 border="true"
 follows="all"
 height="408"
 layout="topleft"
 left="1"
 top="1"
 width="517"
 label="Text Chat"
 name="chat">

<tab_container
 label="Firestorm Prefs"
 layout="topleft"
 follows="all"
 top="5"
 bottom="-1"
 left="1"
 right="-1"
 name="tabs"
 tab_min_width="50"
 tab_position="top" >

        <!-- Chat: General -->
        <panel
         top_pad="1"
         bottom="-1"
         left="1"
         right="-1"
         follows="all"
         label="General"
         name="ChatGeneral" >

  <check_box
      control_name="ChatAutocompleteGestures"
      height="16"
      initial_value="true"
      label="Auto-complete gestures in nearby chat"
      layout="topleft"
      top="10"
      left="20"
      name="auto_complete_gestures"
      width="330">
  </check_box>

    <check_box
        control_name="PlayTypingAnim"
        height="16"
        initial_value="true"
        label="Play typing animation when chatting"
        layout="topleft"
	      left="20"
        name="play_typing_animation"
        width="330">
    </check_box>
    <check_box
        control_name="PlainTextChatHistory"
        height="16"
        initial_value="false"
        label="Display plain text chat"
        layout="topleft"
        name="plain_test_chat_history"
        width="330">
    </check_box>
    <check_box
        enabled="false"
        height="16"
        label="Only friends and groups can call or IM me"
        layout="topleft"
        name="voice_call_friends_only_check"
        width="350">     
    </check_box>
    <text
       font="SansSerifSmall"
       height="16"
       layout="topleft"
       length="1"       
       name="email_settings"
       skip_link_underline="true"
       left_delta="2"
       type="string"
       width="350">
      [https://accounts.secondlife.com/change_email Email me IMs when I'm offline]
    </text>
    <text
        layout="topleft"
        follows="left|top"
        left="24"
        top_pad="10"
        height="12"
        name="font_size"
        width="120">
      Font size:
    </text>
    <combo_box
        control_name="ChatFontSize"
        height="23"
        layout="topleft"
        follows="left|top"
        left="80"
        name="chat_font_size"
        top_pad="-18"
        width="100">
      <item
          label="Small"
          name="Small"
          value="0"/>
      <item
          label="Medium"
          name="Medium"
          value="1"/>
      <item
          label="Large"
          name="Large"
          value="2"/>  
    </combo_box>
    <check_box
        control_name="UseChatBubbles"
        height="16"
        label="Bubble Chat"
        follows="top|left"
        layout="topleft"
        top_pad="4"
        left="20"
        name="bubble_text_chat"
        width="330">
    </check_box>
    <check_box
        control_name="KokuaSuppressBubbleChatOverridingToasts"
        height="16"
        label="Bubble Chat does not suppress nearby avatar chat notifications"
        follows="top|left"
        layout="topleft"
        top_pad="4"
        left="20"
        name="kokua_suppress_bubble_chat_overriding_toasts"
        width="330">
    </check_box>
    <check_box
        control_name="CloseChatOnReturn"
        height="16"
        label="Conversations:Nearby Chat/Chat Bar release keyboard focus after sending a line of chat"
        follows="top|left"
        layout="topleft"
        top_pad="4"
        left="20"
        name="close_chat_on_return"
        width="330">
    </check_box>
    <check_box
        control_name="KokuaCloseChatBarOnReturn"
        height="16"
        label="Close the Chat Bar after sending a line of chat"
        follows="top|left"
        layout="topleft"
        top_pad="4"
        left="20"
        name="kokua_close_chat_bar_on_return"
        width="330">
    </check_box>
    <check_box
        control_name="KokuaReopenChatBarAtLogin"
        height="16"
        label="Always open the Chat Bar at login"
        follows="top|left"
        layout="topleft"
        top_pad="4"
        left="20"
        name="kokua_reopen_chat_bar_at_login"
        width="330">
    </check_box>
    <check_box
        control_name="KokuaIsTypingNotificationInConversationsTabs"
        height="16"
        label="Add a typing indication in Conversations Floater when other party is typing in IM"
        follows="top|left"
        layout="topleft"
        top_pad="4"
        left="20"
        name="kokua_is_typing_notification_in_conversations_tabs"
        width="330">
    </check_box>
    <check_box
        control_name="KokuaPreventChatShout"
        height="16"
        label="Disable the CTRL to shout feature in chat entry fields"
        follows="top|left"
        layout="topleft"
        top_pad="4"
        left="20"
        name="kokua_prevent_chat"
        width="330">
    </check_box>
    <text
        layout="topleft"
        left="24"
        name="play_sound"
        width="100"
        follows="top|left"
        top_pad="8"
        visible="true">
      Play sound:
    </text>
    <check_box
        control_name="PlaySoundNewConversation"
        height="16"
        label="New conversation"
        layout="topleft"
        left_pad="15"
        top_pad="-10"
        name="new_conversation"
        width="150" />
    <check_box
        control_name="PlaySoundIncomingVoiceCall"
        height="16"
        label="Incoming voice call"
        layout="topleft"
        top_pad="6"
        name="incoming_voice_call"
        width="150" />
    <check_box
        control_name="PlaySoundTeleportOffer"
        height="16"
        label="Teleport offer"
        layout="topleft"
        left_pad="35"
        top_pad="-38"
        name="teleport_offer"
        width="150" />
    <check_box
        control_name="PlaySoundInventoryOffer"
        height="16"
        label="Inventory offer"
        layout="topleft"
        top_pad="6"
        name="inventory_offer"
        width="150" />
    <text
        name="logging_label"
        layout="topleft"
        left="24"
        follows="top|left"
        top_pad="8"
        height="12"
        width="55">
        Save:
    </text>
    <combo_box
        enabled="false"
        control_name="KeepConversationLogTranscripts"
        height="23"
        layout="topleft"
        follows="top|left"
        left_pad="0"
        top_pad="-16"
        name="conversation_log_combo"
        width="165">
        <item
            name="log_and_transcripts"
            label="Log and transcripts"
            value="2"/>
        <item
            name="log_only"
            label="Log only"
            value="1"/>
        <item
            name="no_log_or_transcript"
            label="No log or transcripts"
            value="0"/>
    </combo_box>
    <button
        enabled="false"
        height="23"
        label="Clear log..."
        layout="topleft"
        follows="top|left"
        left_pad="5"
        name="clear_log"
        width="110">
        <commit_callback
            function="Pref.ClearLog" />
    </button>  
    <button
        enabled="false"
        height="23"
        label="Delete transcripts..."
        layout="topleft"
        left_pad="5"
        follows="top|left"
        name="delete_transcripts"
        width="147">
        <button.commit_callback
            function="Pref.DeleteTranscripts" />
    </button> 
    <text
        name="log_location_label"
        layout="topleft"
        follows="top|left"
        left="24"
        height="12"
        top_pad="15"
        width="55">
        Location:
    </text>  
    <line_editor
        control_name="InstantMessageLogPath"
        border_style="line"
        border_thickness="1"
        font="SansSerif"
        height="23"
        layout="topleft"
        follows="top|left"
        left_pad="55"
        max_length_bytes="4096"
        name="log_path_string"
        top_delta="-5"
        width="185">
    </line_editor> 
    <button
        enabled="false"
        follows="left|top"
        height="23"
        label="Browse..."
        label_selected="Browse"
        layout="topleft"
        left_pad="5"
        name="log_path_button"
        top_delta="0"
        width="112">
      <commit_callback function="Pref.LogPath" />
    </button>
  
  <button
      height="23"
      label="Translation..."
      layout="topleft"
      left="9"
      name="ok_btn"
      top="-29"
      width="170">
    <commit_callback
        function="Pref.TranslationSettings" />
  </button>
  <button
      height="23"
      layout="topleft"
      top_pad="-23"
      left_pad="5"
      name="autoreplace_showgui"
      commit_callback.function="Pref.AutoReplace"
      label="Auto-Replace..."
      width="150">
  </button>
  <button
      height="23"
      layout="topleft"
      top_pad="-23"
      left_pad="5"
      name="spellcheck_showgui"
      commit_callback.function="Pref.SpellChecker"
      label="Spell Checking..."
      width="150">
  </button>
         
        </panel>

